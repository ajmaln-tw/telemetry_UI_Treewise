{
    "MuiButtonBase": {
        "styleOverrides": {
            "switchBase": {
                "borderRadius": "15px",
                "height": "25px",
                "fontWeight": 600,
                "color": "#ccc",
                "backgroundColor": "#f8f8f8",
                "&.Mui-checked": {
                    "color": "#f2ff00"
                }
            },
            "track": {
                "opacity": 0.2,
                "backgroundColor": "#fff",
                ".Mui-checked.Mui-checked + &": {
                    "opacity": 0.7,
                    "backgroundColor": "#fff"
                }
            }
        }
    }
}
{
    "MuiOutlinedInput": {
        "styleOverrides": {
            "root": {
                "multiline": "true",
                "fontSize": "12px",
                "borderRadius": "5px",
                "height": "25px",
                "fontWeight": 600,
                "color": "#000",
                "backgroundColor": "#f8f8f8",
                "fieldset": {
                    "border": "1px solid #E0F8E2 !important"
                },
                " &:hover fieldset": {
                    "border": "1px solid #0784D6 !important"
                }
            }
        }
    }
}